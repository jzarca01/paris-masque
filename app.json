{
  "expo": {
    "name": "Paris Masqué",
    "slug": "paris-masque",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.zarcainc.parismasque",
      "supportsTablet": false,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationWhenInUseUsageDescription": "Nous avons besoin d'avoir accès à votre position (même lorsque l'app n'est pas active) pour vous prévenir."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBX9vQ8Vor-iw4ZZvrTR5STac1f0R9RsiQ"
      }
    },
    "android": {
      "package": "com.zarcainc.parismasque",
      "permissions": ["NOTIFICATIONS", "LOCATION"],
      "versionCode": 2,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBX9vQ8Vor-iw4ZZvrTR5STac1f0R9RsiQ"
        }
      }
    },
    "description": ""
  }
}
